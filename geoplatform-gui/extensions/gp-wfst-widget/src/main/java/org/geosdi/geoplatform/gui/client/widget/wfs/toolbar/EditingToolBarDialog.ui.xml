<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:wfsEdit='urn:org.geosdi.geoplatform.gui.client.widget.wfs.toolbar'>

    <ui:style>
        .panel {
        position: absolute;
        top: 70px;
        padding: 0 12px;
        width: 50px;
        height: 300px!important;
        overflow-x: auto;
        z-index: 1000!important;
        } 
    </ui:style>

    
    <g:FlowPanel ui:field="toolbarEditorPanel" styleName="{style.panel}" />
    
</ui:UiBinder>
